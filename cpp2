#include<iostream>
#include<math.h>
using namespace std;

int main()
{	
	float tienkivong;
  float nam;
  float tienvon;
  float laisuat;
  float a,b;
	cout<<"nhap so tien ki vong: ";
	cin>>tienkivong;
	cout<<"nhap so tien von: ";
	cin>>tienvon;
	cout<<"so lai suat: ";
	cin>>laisuat;
	if(tienkivong>tienvon)
	{
	laisuat=float(laisuat/100);
	a=float(tienkivong/tienvon);
	b=float(1+laisuat);
	nam=float(log(a)/log(b));
	cout<<"so nam ki vong la: "<<nam;
	}
	else
	cout<<"khong hop le";
    
	return 0;

}
